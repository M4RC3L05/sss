{
  "name": "sss",
  "version": "0.0.0",
  "type": "module",
  "main": "./src/app.js",
  "exports": {
    ".": "./src/app.js",
    "./middlewares": "./src/middlewares/mod.js"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "editorconfig": true,
    "proseWrap": "preserve",
    "printWidth": 100,
    "quoteProps": "as-needed",
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "xo": {
    "env": [
      "node"
    ],
    "extensions": [
      ".ts"
    ],
    "prettier": true,
    "rules": {
      "func-names": "off",
      "@typescript-eslint/promise-function-async": "off"
    }
  },
  "scripts": {
    "transpile": "tsc"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "prettier": "^3.0.3",
    "xo": "^0.56.0"
  },
  "dependencies": {
    "find-my-way": "^7.7.0"
  }
}
