{
  "name": "sss",
  "version": "0.0.0",
  "type": "module",
  "main": "./src/app.js",
  "exports": {
    ".": "./src/app.js",
    "./middlewares": "./src/middlewares/mod.js"
  },
  "scripts": {
    "generate:dts": "rm -rf ./src/*.d.ts && rm -rf ./src/**/*.d.ts && tsc"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "editorconfig": true,
    "proseWrap": "preserve",
    "printWidth": 100,
    "quoteProps": "as-needed",
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "xo": {
    "env": ["node"],
    "prettier": true,
    "rules": {
      "func-names": "off"
    }
  },
  "devDependencies": {
    "prettier": "^3.0.3",
    "xo": "^0.56.0"
  }
}
